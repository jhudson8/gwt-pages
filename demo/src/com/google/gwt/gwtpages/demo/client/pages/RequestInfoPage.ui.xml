<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:FlowPanel>
		<g:SimplePanel ui:field="sourceRef"></g:SimplePanel>
		<g:HTMLPanel>
			<br/>
			<div class="section">
				<table>
					<tr>
						<td>History token</td>
						<td><g:Label ui:field="historyToken"></g:Label></td>
					</tr>
					<tr>
						<td>Page token</td>
						<td><g:Label ui:field="pageToken"></g:Label></td>
					</tr>
					<tr>
						<td>Ordinal parameters</td>
						<td><g:Label ui:field="ordinalParameters"></g:Label></td>
					</tr>
					<tr>
						<td>Mapped parameters</td>
						<td><g:Label ui:field="mappedParameters"></g:Label></td>
					</tr>
					<tr>
						<td>Transient parameters</td>
						<td><g:Label ui:field="transientParameters"></g:Label></td>
					</tr>
				</table>
			</div>
			<p>
				<g:Anchor ui:field="gotoPage" styleName="link"></g:Anchor>
			</p>
		</g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder>