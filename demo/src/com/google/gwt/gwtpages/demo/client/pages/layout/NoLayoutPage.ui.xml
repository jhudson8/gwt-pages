<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:DockLayoutPanel unit="PX">
		<g:center>
			<g:ScrollPanel>
				<g:FlowPanel>
					<g:Label ui:field="pageTitle" styleName="pageTitle"></g:Label>
					<g:SimplePanel ui:field="messages"></g:SimplePanel>
					<g:SimplePanel ui:field="bodyContent"></g:SimplePanel>
				</g:FlowPanel>
			</g:ScrollPanel>
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>